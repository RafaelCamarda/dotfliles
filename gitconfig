[color "status"]
changed = yellow
added = green
untracked = red

[alias]
l = log --pretty=format:'%C(green)%h%C(reset) %an %C(yellow)%ar%C(reset) %C(magenta)%d %C(reset)%s' --no-pager -30
s = status
d = diff
c = checkout
cb = checkout -b
rb = branch -d
dc = diff --cached
lg = "log --graph --pretty=format:'%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"


[core]
editor = nano
